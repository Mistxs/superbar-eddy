{
  "manifest_version": 2,
  "name": "suberbar",
  "version": "1.0",
  
  "description": "Расширение для переопределения стилей на сайте yclients.helpdeskeddy.com",
  "permissions": ["https://yclients.helpdeskeddy.com/*"],
  "content_scripts": [
    {
